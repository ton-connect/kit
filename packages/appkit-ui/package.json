    "TON",
    "Wallet",
    "ton-connect",
    "tonconnect",
    "Connect",
    "Tonkeeper",
    "sdk",
    "UI"
  ],
  "author": "tonconnect",
  "license": "Apache-2.0",
  "files": [
    "lib",
    "dist"
  ],
  "main": "./lib/index.js",
