    "solid-floating-ui": "^0.3.1",
    "solid-js": "^1.9.7",
    "solid-styled-components": "^0.28.5",
    "solid-transition-group": "^0.3.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.2",
