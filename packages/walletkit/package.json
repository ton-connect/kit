{
  "name": "@ton/walletkit",
  "version": "0.0.6",
  "description": "Wallet kit for TON Connect",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ton-connect/kit"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./bridge": {
      "import": {
        "types": "./dist/esm/bridge/JSBridgeInjector.d.ts",
        "default": "./dist/esm/bridge/JSBridgeInjector.js"
      },
      "require": {
        "types": "./dist/cjs/bridge/JSBridgeInjector.d.ts",
        "default": "./dist/cjs/bridge/JSBridgeInjector.js"
      }
    }
  },
  "scripts": {
    "build": "pnpm build:clean && pnpm build:cjs && pnpm build:esm",
    "build:clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "stryker run stryker.config.js",
    "quality": "pnpm test:coverage",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "clean": "git clean -xdf dist node_modules .turbo coverage",
    "generate-openapi-spec": "src/api/scripts/generate-openapi-spec.sh",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ton",
    "wallet",
    "connect",
    "tonconnect"
  ],
  "author": "TON Tech",
  "files": [
    "DEVELOPMENT.md",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "dependencies": {
    "@noble/curves": "^1.9.7",
    "@scure/bip39": "^2.0.1",
    "@tonconnect/bridge-sdk": "https://github.com/ton-connect/bridge-sdk/releases/download/v0.2.6/tonconnect-bridge-sdk-0.2.6.tgz",
    "@tonconnect/protocol": "^2.3.0",
    "@tonconnect/sdk": "^3.3.1",
    "lru-cache": "^11.2.4",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@ton-community/tlb-runtime": "1.0.0-alpha.3",
    "@ton/core": "^0.63.0",
    "@ton/crypto": "^3.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@stryker-mutator/core": "^9.4.0",
    "@ton-community/tlb-runtime": "1.0.0-alpha.3",
    "@ton/core": "^0.63.0",
    "@ton/crypto": "^3.3.0",
    "@types/chrome": "^0.1.33",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "^4.0.17",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "node-hid": "^3.2.0",
    "rimraf": "^6.1.2",
    "swagger-typescript-api": "13.2.16",
    "ts-jest": "^29.4.6",
    "ts-json-schema-generator": "^2.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-bundle-analyzer": "^1.3.2",
    "vitest": "^4.0.17"
  }
}
