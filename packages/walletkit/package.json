{
  "name": "@ton/walletkit",
  "version": "0.1.0",
  "description": "Wallet kit for TON Connect",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./bridge": {
      "import": {
        "types": "./dist/esm/bridge/JSBridgeInjector.d.ts",
        "default": "./dist/esm/bridge/JSBridgeInjector.js"
      },
      "require": {
        "types": "./dist/cjs/bridge/JSBridgeInjector.d.ts",
        "default": "./dist/cjs/bridge/JSBridgeInjector.js"
      }
    }
  },
  "scripts": {
    "build": "pnpm build:clean && pnpm build:cjs && pnpm build:esm",
    "build:clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "stryker run stryker.config.js",
    "quality": "pnpm test:coverage",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix"
  },
  "keywords": [
    "ton",
    "wallet",
    "connect",
    "tonconnect"
  ],
  "author": "",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@noble/curves": "^1.9.7",
    "@ton-community/assets-sdk": "^0.0.5",
    "@ton-community/tlb-runtime": "^1.0.0-alpha.2",
    "@ton/core": "^0.61.0",
    "@ton/crypto": "^3.3.0",
    "@tonconnect/bridge-sdk": "https://github.com/ton-connect/bridge-sdk/releases/download/v0.2.6/tonconnect-bridge-sdk-0.2.6.tgz",
    "@tonconnect/protocol": "^2.3.0",
    "@tonconnect/sdk": "^3.2.0",
    "@truecarry/tlb-abi": "^0.2.0",
    "bip39": "^3.1.0",
    "lru-cache": "^11.1.0",
    "node-hid": "^3.2.0",
    "uuid": "^13.0.0",
    "vite": "^7.1.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stryker-mutator/core": "^8.0.0",
    "@types/chrome": "^0.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "2.1.9",
    "@vitest/ui": "^2.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "vite-bundle-analyzer": "^1.2.1",
    "vitest": "^2.0.0"
  }
}
