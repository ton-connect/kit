{
  "name": "@ton/walletkit",
  "version": "0.1.0",
  "description": "Wallet kit for TON Connect",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./bridge": {
      "import": {
        "types": "./dist/esm/bridge/JSBridgeInjector.d.ts",
        "default": "./dist/esm/bridge/JSBridgeInjector.js"
      },
      "require": {
        "types": "./dist/cjs/bridge/JSBridgeInjector.d.ts",
        "default": "./dist/cjs/bridge/JSBridgeInjector.js"
      }
    }
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm",
    "build:clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix"
  },
  "keywords": [
    "ton",
    "wallet",
    "connect",
    "tonconnect"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@noble/curves": "^1.9.7",
    "@ton-community/assets-sdk": "^0.0.5",
    "@ton-community/tlb-runtime": "^1.0.0-alpha.2",
    "@ton/core": "^0.61.0",
    "@ton/crypto": "^3.3.0",
    "@ton/ton": "^15.3.1",
    "@tonconnect/bridge-sdk": "https://github.com/ton-connect/bridge-sdk/releases/download/v0.2.6-alpha.1/tonconnect-bridge-sdk-0.2.6-alpha.1+fetch.tgz",
    "@tonconnect/protocol": "^2.3.0",
    "@tonconnect/sdk": "^3.2.0",
    "@truecarry/tlb-abi": "^0.2.0",
    "lru-cache": "^11.1.0",
    "vite": "^7.1.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.4",
    "@types/node": "^24.3.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "vite-bundle-analyzer": "^1.2.1"
  }
}
