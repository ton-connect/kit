{
  "name": "@ton/appkit-react",
  "version": "0.0.1-alpha.2",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ton-connect/kit"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./tonconnect": {
      "types": "./dist/tonconnect/tonconnect-bridge.d.ts",
      "require": "./dist/tonconnect/tonconnect-bridge.js",
      "import": "./dist/tonconnect/tonconnect-bridge.mjs",
      "default": "./dist/tonconnect/tonconnect-bridge.js"
    },
    "./styles.css": "./dist/styles/index.css"
  },
  "dependencies": {
    "@ton/appkit": "workspace:*",
    "clsx": "2.1.1",
    "radix-ui": "^1.4.3",
    "rosetta": "1.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "10.2.8",
    "@storybook/react": "10.2.8",
    "@storybook/react-vite": "10.2.8",
    "@storybook/test": "^8.6.15",
    "@tanstack/react-query": "catalog:",
    "@tonconnect/ui-react": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "storybook": "10.2.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "vite-plugin-node-polyfills": "^0.25.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "@tonconnect/ui": ">=2.4.1",
    "@tonconnect/ui-react": ">=2.4.1",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@tonconnect/ui": {
      "optional": true
    },
    "@tonconnect/ui-react": {
      "optional": true
    }
  },
  "sideEffects": [
    "*.css"
  ]
}
