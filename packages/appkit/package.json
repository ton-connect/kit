{
  "name": "@ton/appkit",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./tonconnect": {
      "import": {
        "types": "./dist/connectors/tonconnect/index.d.ts",
        "default": "./dist/connectors/tonconnect/index.js"
      },
      "require": {
        "types": "./dist/connectors/tonconnect/index.d.cts",
        "default": "./dist/connectors/tonconnect/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "tonconnect": [
        "./dist/connectors/tonconnect/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ton/walletkit": "workspace:*"
  },
  "peerDependencies": {
    "@ton/core": "^0.62.0 || ^0.63.0",
    "@tonconnect/protocol": "catalog:",
    "@tonconnect/sdk": "catalog:"
  },
  "peerDependenciesMeta": {
    "@tonconnect/protocol": {
      "optional": true
    },
    "@tonconnect/sdk": {
      "optional": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@ton/core": "catalog:",
    "@tonconnect/protocol": "catalog:",
    "@tonconnect/sdk": "catalog:",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "vitest": "^4.0.17"
  }
}
