{
  "name": "@ton/appkit",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./tonconnect": {
      "import": {
        "types": "./dist/providers/tonconnect/index.d.ts",
        "default": "./dist/providers/tonconnect/index.js"
      },
      "require": {
        "types": "./dist/providers/tonconnect/index.d.cts",
        "default": "./dist/providers/tonconnect/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "tonconnect": ["./dist/providers/tonconnect/index.d.ts"]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ton-community/assets-sdk": "^0.0.5",
    "@ton/core": "^0.61.0",
    "@ton/walletkit": "workspace:*",
    "@tonconnect/protocol": "catalog:",
    "@tonconnect/sdk": "catalog:"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3"
  }
}
