{
  "name": "@ton/appkit",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ton-connect/kit"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./queries": {
      "import": {
        "types": "./dist/queries/index.d.ts",
        "default": "./dist/queries/index.js"
      },
      "require": {
        "types": "./dist/queries/index.d.ts",
        "default": "./dist/queries/index.cjs"
      }
    },
    "./swap/omniston": {
      "import": {
        "types": "./dist/swap/omniston/index.d.ts",
        "default": "./dist/swap/omniston/index.js"
      },
      "require": {
        "types": "./dist/swap/omniston/index.d.ts",
        "default": "./dist/swap/omniston/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "tonconnect": [
        "./dist/connectors/tonconnect/index.d.ts"
      ],
      "queries": [
        "./dist/queries/index.d.ts"
      ],
      "swap/omniston": [
        "./dist/swap/omniston/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup --onSuccess \"tsc --emitDeclarationOnly\"",
    "dev": "pnpm run build --watch --no-clean",
    "quality": "pnpm test",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ton/walletkit": "workspace:*"
  },
  "peerDependencies": {
    "@tanstack/query-core": ">=5.0.0",
    "@ton/core": "^0.62.0 || ^0.63.0",
    "@ton/crypto": "^3.3.0",
    "@tonconnect/ui": ">=2.4.1"
  },
  "peerDependenciesMeta": {
    "@tonconnect/ui": {
      "optional": true
    },
    "@ston-fi/omniston-sdk": {
      "optional": true
    },
    "@tanstack/query-core": {
      "optional": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@tanstack/query-core": "catalog:",
    "@ton/core": "catalog:",
    "@tonconnect/ui": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "vitest": "^4.0.17"
  }
}
