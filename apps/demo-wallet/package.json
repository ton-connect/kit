{
  "name": "demo-wallet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "e2e:deps": "playwright install --with-deps",
    "e2e": "playwright test --config e2e.config.ts",
    "dev": "vite",
    "build": "pnpm build:extension && pnpm build:website",
    "dev:extension": "cross-env VITE_APP_ENV=extension vite build --watch --config vite.extension.config.ts",
    "build:extension": "cross-env VITE_APP_ENV=extension vite build --config vite.extension.config.ts",
    "build:website": "tsc -b && vite build",
    "copy-extension-files": "node -e \"require('fs').copyFileSync('public/manifest.json', 'dist-extension/manifest.json')\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ledgerhq/hw-transport-webhid": "^6.30.6",
    "@ledgerhq/hw-transport-webusb": "^6.29.10",
    "@tailwindcss/vite": "^4.1.12",
    "@ton/core": "^0.61.0",
    "@ton/crypto": "^3.3.0",
    "@ton/v4ledger-adapter": "workspace:*",
    "@ton/walletkit": "workspace:*",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "immer": "^10.1.1",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@types/chrome": "^0.1.4",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "allure-js-commons": "^3.4.1",
    "allure-playwright": "^3.4.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
